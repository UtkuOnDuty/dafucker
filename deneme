        local HatName = game:GetObjects(Hat)[1].Name
        local HatLocation = game.Players.LocalPlayer.Character:WaitForChild(HatName)
        local AccessWeld = Instance.new("Weld")
        AccessWeld.Name = "AccessoryWeld"
        AccessWeld.Parent = HatLocation
        AccessWeld.Part0 = Plr.Head
        AccessWeld.Part1 = HatLocation.Handle
        HatLocation.Handle.SpecialMesh.Offset = HatOffset
    end)


Section:NewButton("Blue Horn", "Blue Horn giver x2 executer and krnl only", function()


        local HatOffset = Vector3.new(10,10,10.10)
        local Hat = "rbxassetid://74891470"
        local Plr = game.Players.LocalPlayer.Character
        game:GetObjects(Hat)[1].Parent = game.Players.LocalPlayer.Character
        local HatName = game:GetObjects(Hat)[1].Name
        local HatLocation = game.Players.LocalPlayer.Character:WaitForChild(HatName)
        local AccessWeld = Instance.new("Weld")
        AccessWeld.Name = "AccessoryWeld"
        AccessWeld.Parent = HatLocation
        AccessWeld.Part0 = Plr.Head
        AccessWeld.Part1 = HatLocation.Handle
        HatLocation.Handle.SpecialMesh.Offset = HatOffset
    end)


